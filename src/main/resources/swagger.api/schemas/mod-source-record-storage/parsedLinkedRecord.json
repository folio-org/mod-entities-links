{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Parsed record",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "description": "UUID",
      "type": "string",
      "format": "uuid"
    },
    "content": {
      "description": "Parsed record content, e.g. MARC record",
      "properties": {
        "fields": {
          "description": "External id",
          "type": "object",
          "title": "FieldContent",
          "additionalProperties": {
            "type": "object",
            "properties": {
              "ind1": {
                "type": "string"
              },
              "ind2": {
                "type": "string"
              },
              "subfields": {
                "type": "object",
                "additionalProperties": {
                  "type": "string"
                }
              },
              "authorityId": {
                "type": "string",
                "format": "uuid"
              },
              "naturalId": {
                "type": "string"
              },
              "linksStatus": {
                "type": "string",
                "enum": [
                  "NEW",
                  "ACTUAL",
                  "ERROR"
                ]
              },
              "ruleId": {
                "type": "integer"
              },
              "errorStatusCode": {
                "type": "string"
              }
            }
          }
        },
        "leader": {
          "description": "Parsed record content, e.g. MARC record",
          "type": "string"
        }
      }
    }
  },
  "required": [
    "content"
  ]
}