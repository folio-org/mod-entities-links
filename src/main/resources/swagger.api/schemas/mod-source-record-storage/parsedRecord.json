{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Parsed record",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "id": {
      "description": "UUID",
      "type": "string",
      "format": "uuid"
    },
